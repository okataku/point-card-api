# Ignore bundler config.
/point-card-api/.bundle

# Ignore all logfiles and tempfiles.
/point-card-api/log/*
/point-card-api/tmp/*
!/point-card-api/log/.keep
!/point-card-api/tmp/.keep

# Ignore uploaded files in development
/point-card-api/storage/*
!/point-card-api/storage/.keep

.byebug_history

# Ignore master key for decrypting credentials and more.
/point-card-api/config/master.key

# env
.env

# Docker
/docker/ruby/bundle

# Docs
/docs/client_errors.md
/docs/paths.md
/docs/requests.md

# Etc
.DS_Store